{
  "src":"./my-llvm-src",
  "repo":
  {
    "llvm":{
      "url":"https://github.com/llvm-mirror/llvm.git",
      "branch":"master"
    },
    "clang":{
      "url":"https://github.com/llvm-mirror/clang.git",
      "branch":"master"
    },
    "compiler-rt":{
      "url":"https://github.com/llvm-mirror/compiler-rt.git",
      "branch":"master"
    },
    "libcxx":{
      "url":"https://github.com/llvm-mirror/libcxx.git",
      "branch":"master"
    },
    "libcxxabi":{
      "url":"https://github.com/llvm-mirror/libcxxabi.git",
      "branch":"master"
    }
  },
  "builds":{
    "release":{
      "path":"./my-llvm-release",
      "sharedlib":false
    },
    "relassert":{
      "path":"./my-llvm-releaseassert",
      "sharedlib":true
    },
    "debug":{
      "path":"./my-llvm-debug",
      "sharedlib":true
    }
  }
}
