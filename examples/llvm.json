{
  "src":"./my-llvm-project",
  "repo":"git@github.com:llvm/llvm-project.git",
  "name":"my-llvm-trunk",
  "branch":"main",
  "builds":{
    "release":{
      "path":"./my-llvm-release",
      "projects":"llvm;clang;libcxx;libcxxabi;compiler-rt",
      "sharedlib":false,
      "cc":"gcc",
      "cxx":"g++"
    },
    "relassert":{
      "path":"./my-llvm-releaseassert",
      "projects":"llvm;clang;libcxx;libcxxabi;compiler-rt",
      "sharedlib":true,
      "cc":"gcc",
      "cxx":"g++"
    },
    "debug":{
      "path":"./my-llvm-debug",
      "projects":"llvm",
      "sharedlib":true,
      "cc":"gcc",
      "cxx":"g++"
    }
  }
}
